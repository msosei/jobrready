# Frontend environment variables for MyBrand Job Application Platform
# Copy this file to .env.development and fill in your actual values

# Client target URL for Encore services
# This is used for connecting to the Encore backend services
VITE_CLIENT_TARGET=http://localhost:4000

# API base URL for backend services
# This is the base URL for all API calls to the FastAPI backend
VITE_API_URL=http://localhost:8000

# Supabase configuration for authentication and database services
# These values are required for user authentication and data storage
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key-here